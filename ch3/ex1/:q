#include <stdio.h>

int binsearch(int x, int v[], int n);

int main() {
  int a[] = [0, 1, 2, 3, 4, 5];
  printf("should be %d: %d\n", 3, binsearch(3, a, 6));

  int b[] = [0, 1, 2, 3, 4, 5];
  printf("should be %d: %d\n", -1, binsearch(6, b, 6));

  int c[] = [100, 200, 250, 275, 280, 300, 301, 1000, 100000];
  printf("should be %d: %d\n", 0, binsearch(100, c, 9));

  int d[] = [100, 200, 250, 275, 280, 300, 301, 1000, 100000];
  printf("should be %d: %d\n", 8, binsearch(100000, d, 9));

  return 0;
}

int binsearch(int needle, int haystack[], int len) {
  return 1;
}

